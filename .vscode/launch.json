    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:8000/",
            "webRoot": "${workspaceFolder}"
        },
        
        {
            "type": "msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "http://127.0.0.1:5174/",
            "webRoot": "${workspaceFolder}"
        }
    ]
}


